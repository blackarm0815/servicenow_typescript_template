{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "rules": {
        "func-names": 0,
        "max-len": 0,
        "no-underscore-dangle": 0,
        "prefer-arrow-callback": 0,
        "prefer-destructuring": 0,
        "no-var": 0,
        "object-shorthand": 0,
        "wrap-iife": 0
    }
}